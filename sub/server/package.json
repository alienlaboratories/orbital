{
  "name": "orbital-server",
  "version": "0.0.1",
  "description": "Orbital Test Server.",
  "license": "UNLICENSED",
  "repository": "https://github.com/alienlabs/orbital",
  "main": "src/main.js",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^21.0.2",
    "babel-plugin-inline-import": "^2.0.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "regenerator-runtime": "^0.11.0"
  },
  "dependencies": {
    "express": "^4.15.4"
  },
  "jest": {
    "verbose": false,
    "cacheDirectory": "/tmp/jest",
    "transformIgnorePatterns": [
      "/node_modules/(?!orbital-)"
    ]
  },
  "scripts": {
    "lint": "eslint -c ../../eslintrc.yml ./src",
    "test": "jest --no-cache --silent",
    "server": "babel-node ./src/main.js",
    "start": "hilite nodemon --exec npm run server"
  }
}
